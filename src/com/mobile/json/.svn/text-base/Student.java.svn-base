package com.mobile.json;

public class Student{
	
       private long id;
       private String name;
       private long age;
       private String eamil;
       

	
	public Student() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Student(long l, String name, long m, String eamil) {
		super();
		this.id = l;
		this.name = name;
		this.age = m;
		this.eamil = eamil;
	}
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public long getAge() {
		return age;
	}
	public void setAge(long age) {
		this.age = age;
	}
	public String getEamil() {
		return eamil;
	}
	public void setEamil(String eamil) {
		this.eamil = eamil;
	}
	public String toString() {
		return "Student [age=" + age + ", eamil=" + eamil + ", id=" + id
				+ ", name=" + name + "]";
	}
       
       
}
